<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
      <servlet>
   <servlet-name>publish</servlet-name>
    <servlet-class>com.ldr.servlet.PublishServet</servlet-class>
     
  </servlet>
  <servlet-mapping>
    <servlet-name>publish</servlet-name>
    <url-pattern>/publish</url-pattern>
     </servlet-mapping>
     
     
        <servlet>
   <servlet-name>Register</servlet-name>
    <servlet-class>com.ldr.servlet.RegisterServet</servlet-class>
     
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
     </servlet-mapping>
     
     
     
           <servlet>
   <servlet-name>login</servlet-name>
    <servlet-class>com.ldr.servlet.LoginServet</servlet-class>
     
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
     </servlet-mapping>
     
       <servlet>
   <servlet-name>Article</servlet-name>
    <servlet-class>com.ldr.servlet.ArticleServlet</servlet-class>
     
  </servlet>
  <servlet-mapping>
    <servlet-name>Article</servlet-name>
    <url-pattern>/article</url-pattern>
     </servlet-mapping>
     
   

</web-app>
